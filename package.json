{
    "name": "@oniria/tikitup-api",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "debug": "sam local start-api --warm-containers EAGER -t template.yaml --host 0.0.0.0 --port 8393 2>&1 | tr \"\r\" \"\n\"",
        "format": "prettier --write \"packages/**/*.ts\"",
        "format:fix": "pretty-quick --pattern \"packages/**/*.ts\" --staged",
        "lint": "eslint --ignore-path .eslintignore ./packages/**/*.ts",
        "lint:fix": "yarn run lint --fix",
        "prepare": "husky install"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.850.0",
        "@onesignal/node-onesignal": "^5.0.0-alpha-01",
        "@paypal/paypal-server-sdk": "1.1.0",
        "aws-lambda": "^1.0.7",
        "aws-sdk": "^2.1390.0",
        "http-status-codes": "^2.3.0",
        "jsonwebtoken": "^9.0.0",
        "mariadb": "^3.1.2",
        "paypal-rest-sdk": "^1.8.1",
        "qrcode": "^1.5.3",
        "stripe": "^18.2.1",
        "uuid": "^9.0.0",
        "xml2js": "^0.6.2",
        "xmlbuilder2": "^3.1.1"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.114",
        "@types/jsonwebtoken": "^9",
        "@types/node": "^18.15.11",
        "@types/paypal-rest-sdk": "^1.7.7",
        "@types/qrcode": "^1",
        "@types/uuid": "^9",
        "@types/xml2js": "^0",
        "@typescript-eslint/eslint-plugin": "^5.57.1",
        "@typescript-eslint/parser": "^5.57.1",
        "eslint": "^8.37.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.3",
        "prettier": "2.8.7",
        "pretty-quick": "^3.1.3",
        "typescript": "^5.0.3"
    },
    "packageManager": "yarn@3.5.0"
}
